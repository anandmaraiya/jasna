#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/coffee" ]; then
  "$basedir/coffee"  "$basedir/../fs-extra/bin/TEST_file-extra" "$@"
  ret=$?
else 
  coffee  "$basedir/../fs-extra/bin/TEST_file-extra" "$@"
  ret=$?
fi
exit $ret
